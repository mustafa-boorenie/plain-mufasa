        <img
          src="{{ settings.collection_screen_logo | image_url }}"
          style="width: {{ settings.collection_screen_logo_size }}px"
          loading="lazy"
          alt=""
          class="logo"
        >
      {% else %}
        <img src="{{ 'Supreme-dummy.png' | asset_url }}" loading="lazy" alt="" class="logo">
      {% endif %}
    </a>
      {% if section.settings.show_date_time %}
    <div class="date-time-container">
        <div class="date" style="color: {{ section.settings.mobile_index_date_color }}">{{ 'tn7d8e379f' | t }}</div>
        <div class="time" style="color: {{ section.settings.mobile_index_time_color }}">{{ 't589b2285' | t }}</div>
    </div>
        {% endif %}
